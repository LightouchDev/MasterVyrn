{
  "env": {
    "test": {
      "presets": [
        ["env", {
          "targets": { "node": 8 }
        }],
        "stage-0"
      ],
      "plugins": ["istanbul"]
    },
    "main": {
      "presets": [
        ["env", {
          "targets": { "node": 8 }
        }],
        "stage-0"
      ]
    },
    "renderer": {
      "presets": [
        ["env", {
          "modules": false
        }],
        "stage-0"
      ]
    },
    "preload": {
      "presets": [
        ["env", {
          "targets": { "node": 8 }
        }],
        "stage-0"
      ]
    }
  },
  "plugins": ["transform-runtime", "syntax-dynamic-import", "lodash"]
}
